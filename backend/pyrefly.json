{
  "include": [
    "api",
    "backend"
  ],
  "exclude": [
    "**/migrations",
    "**/__pycache__",
    "**/.venv",
    "**/venv"
  ],
  "venvPath": "..",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "typeCheckingMode": "standard",
  "diagnostics": {
    "enabled": true
  },
  "completion": {
    "enabled": true
  }
}