{
  "include": [
    "api",
    "backend"
  ],
  "exclude": [
    "**/migrations",
    "**/__pycache__",
    "**/.venv",
    "**/venv"
  ],
  "venvPath": "..",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}